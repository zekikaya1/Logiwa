{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ProductApiClient": {
    "BaseAddress": "http://logiwa.product.api:8081",
    "Timeout": "50000",
    "RetryCount": "3",
    "RetryDelayInMs": "500",
    "EndPoints": {
      "GetProducts": "/api/products",
      "GetProductById": "/api/products/{0}",
      "CreateProduct": "/api/products",
      "UpdateProduct": "/api/products/{0}",
      "DeleteProduct": "/api/products/{0}",
      "SearchProducts": "/api/products/search"
    }
  },
  
  "CategoryApiClient": {
    "BaseAddress": "http://logiwa.product.api:8081",
    "Timeout": "50000",
    "RetryCount": "3",
    "RetryDelayInMs": "500",
    "EndPoints": {
      "GetCategories": "/api/categories",
      "GetCategoryById": "/api/categories/{0}"
    }
  }
}
